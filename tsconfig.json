{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"outDir": "./server/dist",
		"resolveJsonModule": true,
		"strict": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true
	},
	"exclude": ["node_modules"],
	"include": ["server/src/**/*.ts", "./server.ts"]
}
